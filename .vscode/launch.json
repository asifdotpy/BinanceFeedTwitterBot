{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/BinanceFeedScraper/manage.py",
            "args": [
                "runserver"
            ],
            "django": true,
            "justMyCode": true
        },
        {
            "name": "Python: Django Tests",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/BinanceFeedScraper/manage.py",
            "args": [
                "test",
                "creatorProfile.tests.views_test",
                "--noinput",
                "--verbosity=2",
                "--failfast",
                "--testrunner=xmlrunner.extra.djangotestrunner.XMLTestRunner",
            ],
            "django": true,
            "justMyCode": true,
            "envFile": "${workspaceFolder}/BinanceFeedScraper/creatorProfile/.env"
        }
    ]
}